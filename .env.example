# Frontend Environment Configuration
# Backend: Express.js (Port 3001)

# API Base URL (Express Backend)
VITE_API_BASE_URL="http://127.0.0.1:3001/api"

# Image Base URL (for images served by Express)
VITE_IMAGE_BASE_URL="http://127.0.0.1:3001"

# Storage Base URL (for file downloads)
VITE_STORAGE_BASE_URL="http://127.0.0.1:3001/uploads"

# Laporan Desa API Key (External API)
VITE_LAPORAN_API_KEY="SECRET-KEY"

